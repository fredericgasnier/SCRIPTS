<VirtualHost 192.168.33.50:443>
        ServerName www.jenkins.fred.lan
        ProxyVia On
        ProxyRequests Off
        ProxyPass "/jenkins" "ajp://127.0.0.1:8009/jenkins"
        ProxyPassReverse "/jenkins" "ajp://127.0.0.1:8009/jenkins"
        ProxyPreserveHost on
        ErrorLog /var/log/httpd/www.TonApplication.com-error.log
        LogLevel warn
        CustomLog /var/log/httpd/www.TonApplication.com-access.log combined
  SSLEngine on
  SSLCertificateFile /etc/ssl/FREDhttps.cer
  SSLCertificateKeyFile /etc/ssl/server.key
  SSLVerifyClient require
  SSLVerifyDepth 1
  SSLCACertificateFile /etc/ssl/ca.cer
  ProxyRequests Off
</VirtualHost>
